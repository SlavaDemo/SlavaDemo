---
// Components imports
import BaseLayout from "@layouts/baseLayout.astro";
import SkillSection from "@components/SkillSection.astro";
import { Image } from "@astrojs/image/components";
import {languages, frameworks, clouds, tools} from "src/data/data";
import About from "@components/About.astro";


const defaultStyle = "hidden"
---

<BaseLayout title="Home page" description="This is the Home page">
	<About />
	<SkillSection data={languages} style={defaultStyle} text="Languages I've worked with"/>
	<SkillSection data={frameworks} style={defaultStyle} text="Languages I've worked with"/>
	<SkillSection data={clouds} style={defaultStyle} text="Languages I've worked with"/>
	<SkillSection data={tools} style={defaultStyle} text="Languages I've worked with"/>
	<SkillSection data={languages} style={defaultStyle} text="Languages I've worked with"/>
	<SkillSection data={languages} style={defaultStyle} text="Languages I've worked with"/>
</BaseLayout>

